#!/bin/sh

userresources=$HOME/.Xresources
usermodmap=$HOME/.Xmodmap
sysresources=/etc/X11/xinit/.Xresources
sysmodmap=/etc/X11/xinit/.Xmodmap

export STEAM_RUNTIME=0 &
export TERMINAL=st &
xset m 1/2 1
xrdb ~/.Xresources &
setxkbmap gb &
redshift &
hsetroot -solid #000000 &
feh ~/wallpapers/nebula.tif --bg-fill &
compton &
